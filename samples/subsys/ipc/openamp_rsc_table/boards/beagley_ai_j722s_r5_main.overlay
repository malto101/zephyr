/ {
	chosen {
		zephyr,ipc = &ipc0;
		zephyr,ipc_shm = &ddr0;
	};

	ipc0: ipc {
		compatible = "zephyr,mbox-ipm";
		mboxes = <&mbox 0>, <&mbox 1>;
		mbox-names = "tx", "rx";
		status = "okay";
	};
};
